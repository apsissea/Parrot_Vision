function [rgb] = coloredBboxes(rgb,bboxes,scores,nnet)
%COLOREDBBOXES Summary of this function goes here
%   Detailed explanation goes here

color = {'r','y','g'};

if scores<50
    i = 1;
elseif scores>50 && scores<75
    i = 2;
else
    i = 3;
end

im = imcrop(rgb,bboxes);
label = classify(nnet,imresize(im,[227,227])); % Classify
rgb = insertText(rgb,[bboxes(1) bboxes(2)],char(label),'BoxColor',color{i});
rgb = insertObjectAnnotation(rgb,'rectangle',bboxes,scores,'Color',color{i});

end

